{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "browserstack-build-1",
      "sessionName": "suite_test",
      "debug": true
    }
  },

  "environments": {
    "chrome": {
      "bstack:options": {
        "browserName": "chrome"
      }
    },
    "firefox": {
      "bstack:options": {
        "browserName": "firefox"
      }
    },
    "safari": {
      "bstack:options": {
        "os": "OS X",
        "browserName": "safari"
      }
    },
    "ie": {
      "bstack:options": {
        "browserName": "internet explorer"
      }
    }
  }
}
