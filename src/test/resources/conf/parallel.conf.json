{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "browserstack-build-1",
      "sessionName": "parallel_test",
      "browserVersion": "latest",
      "debug": true
    }
  },

  "environments": {
    "env1": {
      "bstack:options": {
        "browserName": "chrome"
      }
    },
    "env2": {
      "bstack:options": {
        "browserName": "firefox"
      }
    },
    "env3": {
      "bstack:options": {
        "os": "OS X",
        "browserName": "safari"
      }
    }
  }
}
